{
  "author": "your.email@example.com",
  "name": "My Agent",
  "description": "A Bindu AI agent for intelligent task handling",
  "version": "1.0.0",
  "recreate_keys": true,
  "skills": [
    "skills/question-answering",
    "skills/pdf-processing"
  ],
  "capabilities": {},
  "agent_trust": "low",
  "debug_mode": true,
  "debug_level": 2,
  "auth": {
    "enabled": true,
    "provider": "auth0",
    "domain": "dev-tlzrol0zsxw40ujx.us.auth0.com",
    "audience": "https://dev-tlzrol0zsxw40ujx.us.auth0.com/api/v2/",
    "algorithms": [
      "RS256"
    ],
    "issuer": "https://dev-tlzrol0zsxw40ujx.us.auth0.com/",
    "jwks_uri": "https://dev-tlzrol0zsxw40ujx.us.auth0.com/.well-known/jwks.json",
    "require_permissions": false,
    "public_endpoints": [
      "/health",
      "/.well-known/*",
      "/did/resolve*",
      "/agent/info",
      "/agent/skills",
      "/agent/skills/*"
    ],
    "permissions": {
      "message/send": [
        "agent:write"
      ],
      "tasks/get": [
        "agent:read"
      ],
      "tasks/cancel": [
        "agent:write"
      ],
      "tasks/list": [
        "agent:read"
      ],
      "contexts/list": [
        "agent:read"
      ],
      "tasks/feedback": [
        "agent:write"
      ]
    }
  },
  "deployment": {
    "url": "http://0.0.0.0:3773",
    "expose": true,
    "protocol_version": "1.0.0",
    "proxy_urls": [
      "0.0.0.0"
    ],
    "cors_origins": [
      "*"
    ]
  },
  "storage": {
    "type": "memory"
  },
  "scheduler": {
    "type": "memory"
  },
  "oltp_endpoint": [
    "http://jaeger:4318/v1/traces",
    "http://phoenix:6006/v1/traces"
  ],
  "oltp_service_name": "my_bindu_agent",
  "oltp_verbose_logging": true,
  "oltp_service_version": "1.0.0",
  "oltp_deployment_environment": "production",
  "oltp_batch_max_queue_size": 2048,
  "oltp_batch_schedule_delay_millis": 5000,
  "oltp_batch_max_export_batch_size": 512,
  "oltp_batch_export_timeout_millis": 30000,
  "llm_observability": {
    "enabled": true,
    "provider": "phoenix",
    "endpoint": "http://phoenix:6006"
  },
  "num_history_sessions": 5
}
